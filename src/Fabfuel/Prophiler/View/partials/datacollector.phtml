<div id="prophiler-datacollector-<?= md5($dataCollector->getTitle()) ?>" class="datacollector">
    <?php if ($dataCollector instanceof \Fabfuel\Prophiler\DataCollector\RenderableInterface): ?>
        <?= $dataCollector->render() ?>
    <?php else: ?>
    <h2><?= $dataCollector->getIcon() ?> <?= $dataCollector->getTitle() ?></h2>
    <dl class="dl-horizontal">
        <?php foreach ($dataCollector->getData() as $dataName => $data): ?>
            <?php if (empty($data)): continue; endif; ?>
            <?php $this->partial('partials/details/data', ['dataName' => $dataName, 'data' => $data]); ?>
        <?php endforeach; ?>
    </dl>
    <?php endif; ?>
</div>
